class Solution {
    public int maxProfit(int[] prices) {
        int buyPrice = Integer.MAX_VALUE;
        int totalProfit = 0;
        for(int i=0;i<prices.length;i++){
            if(buyPrice < prices[i]){
                int Profit = prices[i] - buyPrice;
                totalProfit =Math.max(Profit,totalProfit);
            }
            else{
                buyPrice = prices[i];
            }
        }
        return totalProfit;
    }
}
